<script>
    $(document).on('click','.upgradesa',function () {
        var id = $(this).data('id');

        $('#myModal').modal({show:true});

    });


    $(document).ready(function(){
        $("#payment_next").on('submit', function(){
            var name = $("#name").val();
            var email = $("#email").val();
            var contact = $("#contact").val();
            $("#submitbut_update").prop("disabled", true);

            var dataString = 'name='+ name + '&email='+ email + '&contact='+ contact;


                // AJAX Code To Submit Form.
                $.ajax({
                    type: "POST",
                    url: "/upgrade",
                    data: dataString,
                    cache: false,
                    success: function(result){

                        $("#submitbut_update").prop("disabled", false);
                        $('#payment_next')[0].reset();
                        $('#myModal').modal('toggle');
                        demo.showNotification('top','right','Submitted! Our executive will be touch with you soon.');
                    }
                });

            return false;
        });
    });
</script>

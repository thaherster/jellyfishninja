<script type="text/javascript">
    $(document).ready(function() {
        // Javascript method's body can be found in assets/js/demos.js
        demo.initDashboardPageCharts();

    });

    // Get the list of Applications
    var getdatss = function getAllDats() {
        $.ajax({
            type: "GET",
            url: "/getdashdata",
            processData: false,
            contentType: false,
            cache: false,
            timeout: 600000,
            success: function (data) {
                console.log("SUCCESS : ", data.deviceCount);
                console.log("SUCCESS : ", data.appCount);
                console.log("SUCCESS : ", data.reportCount);
                console.log("SUCCESS : ", data.openbugCount);
                appc.innerText=""+data.appCount+"/50";
                repc.innerText=""+data.reportCount+"";
                bugc.innerText=""+data.openbugCount+"";
                devc.innerText=""+data.deviceCount+"";


                // return data;
            },
            error: function (e) {

                console.log("ERROR : ", e);
                demo.showNotification('top','right',e.message);
            }
        });
    }

    getdatss();
</script>

